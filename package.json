    "typescript": "^3.7.5",
    "webpack": "^4.41.6"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest",
      "git add"
