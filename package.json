  "version": "1.9.2",
  "scripts": {
    "dev": "next dev -p 5010",
    "start": "next start",
    "build": "NODE_ENV=production next build",
    "deploy:prod": "now --prod && now alias https://dxlab-website.dxlab1.now.sh dxlab.sl.nsw.gov.au",
    "deploy:staging": "now --local-config now.staging.json --confirm --prod",
