{
  "name": "dxlab-website",
  "author": "Kaho Cheung",
  "version": "1.6.0",
  "scripts": {
    "dev": "node server.js -p $PORT",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "start-pm2": "NODE_ENV=production pm2 start server.js --name dxlab-website",
    "deploy:staging": "ansible-playbook deploy/deploy.yml --extra-vars \"stage=staging\" -vvvv",
    "deploy:prod": "ansible-playbook deploy/deploy.yml --extra-vars \"stage=production\" -vvvv",
    "deploy:now-staging": "now --dotenv=.env.now-staging && now alias dxlab-staging",
    "deploy:now-production": "now --dotenv=.env.now-production && now alias dxlab",
    "alias:staging": "now alias staging-dxlab.now.sh -r rules.json",
    "alias:production": "now alias dxlab.now.sh -r rules.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "~10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slnsw/dxlab-website.git"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "helmet": "^3.21.2",
    "http-proxy-middleware": "^0.20.0",
    "isomorphic-unfetch": "^3.0.0",
    "lost": "^8.3.1",
    "next": "^9.2.1",
    "next-routes": "^1.4.2",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "pm2": "^4.2.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-gtm-module": "^2.0.8",
    "react-infinite-scroller": "^1.2.4",
    "react-photoswipe": "^1.3.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "ua-compatible": "0.0.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@slnsw/eslint-config-dxlab": "1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.0.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.3",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.25.0",
    "stylelint": "^13.1.0",
    "stylelint-config-lost": "0.0.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "webpack": "^4.41.6"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest",
      "git add"
    ]
  }
}
