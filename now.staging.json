{
  "name": "dxlab-website-staging",
  "version": 2,
  "scope": "dxlab",
  "env": {
    "DXLAB_WEBSITE_GRAPHQL_URL": "@dxlab_website_staging_graphql_url",
    "DXLAB_WEBSITE_BASE_URL": "@dxlab_website_staging_base_url",
    "DXLAB_WEBSITE_DXLAB_WP_URL": "@dxlab_website_staging_dxlab_wp_url",
    "DXLAB_WEBSITE_FB_APP_ID": "@dxlab_website_prod_fb_app_id",
    "DXLAB_WEBSITE_GTM_ID": "@dxlab_website_prod_gtm_id",
    "DXLAB_WEBSITE_TEST": "@dxlab_website_prod_test"
  },
  "build": {
    "env": {
      "DXLAB_WEBSITE_GRAPHQL_URL": "@dxlab_website_staging_graphql_url",
      "DXLAB_WEBSITE_BASE_URL": "@dxlab_website_staging_base_url",
      "DXLAB_WEBSITE_DXLAB_WP_URL": "@dxlab_website_staging_dxlab_wp_url",
      "DXLAB_WEBSITE_FB_APP_ID": "@dxlab_website_prod_fb_app_id",
      "DXLAB_WEBSITE_GTM_ID": "@dxlab_website_prod_gtm_id",
      "DXLAB_WEBSITE_TEST": "@dxlab_website_prod_test"
    }
  },
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/80hz",
      "destination": "/80hz/"
    },
    {
      "source": "/index",
      "destination": "/index/"
    },
    {
      "source": "/loom",
      "destination": "/loom/"
    },
    {
      "source": "/mainstreet",
      "destination": "/mainstreet/"
    },
    {
      "source": "/meridian",
      "destination": "/meridian/"
    },
    {
      "source": "/muruview",
      "destination": "/muruview/"
    },
    {
      "source": "/off-the-shelf",
      "destination": "/off-the-shelf/"
    },
    {
      "source": "/pano-scope",
      "destination": "/pano-scope/"
    },
    {
      "source": "/portico",
      "destination": "/portico/"
    },
    {
      "source": "/postcards-1001",
      "destination": "/postcards-1001/"
    },
    {
      "source": "/search-terms",
      "destination": "/search-terms/"
    },
    {
      "source": "/weemala",
      "destination": "/weemala/"
    },
    {
      "source": "/youngsydney",
      "destination": "/youngsydney/"
    },
    {
      "source": "/visionaries-explorer",
      "destination": "/visionaries-explorer/"
    },
    {
      "source": "/feed",
      "destination": "/feed/"
    },
    {
      "source": "/young-ct-2018",
      "destination": "/young-ct-2018/"
    }
  ],
  "rewrites": [
    {
      "source": "/80hz/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/80hz/$1"
    },
    {
      "source": "/aereo(.*)",
      "destination": "http://dxlab-fellowship-2019.s3-website-ap-southeast-2.amazonaws.com/aereo$1"
    },
    {
      "source": "(/aereo/data/+)(.*)",
      "destination": "http://dxlab-fellowship-2019.s3-website-ap-southeast-2.amazonaws.com/$2"
    },
    {
      "source": "/api/graphql(.*)",
      "destination": "https://staging-api.dxlab.sl.nsw.gov.au/graphql$1"
    },
    {
      "source": "/files/audio/(.*)",
      "destination": "https://dxlab-website.s3-ap-southeast-2.amazonaws.com/audio/$1"
    },
    {
      "source": "/index/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/index/$1"
    },
    {
      "source": "/loom/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/loom/$1"
    },
    {
      "source": "/kids-audio-guide(.*)",
      "destination": "http://dxlab-kids-audio-guide.s3-website-ap-southeast-2.amazonaws.com/kids-audio-guide$1"
    },
    {
      "source": "/mainstreet/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/mainstreet/$1"
    },
    {
      "source": "/meridian/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/meridian/$1"
    },
    {
      "source": "/muruview/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/muruview/$1"
    },
    {
      "source": "/newselfwales(.*)",
      "destination": "https://dxlab-staging-newselfwales.now.sh/newselfwales$1"
    },
    {
      "source": "/static/newselfwales/(.*)",
      "destination": "https://dxlab-staging-newselfwales.now.sh/static/newselfwales/$1"
    },
    {
      "source": "/off-the-shelf/(.*)",
      "destination": "https://dxlab-staging-off-the-shelf.now.sh/off-the-shelf/$1"
    },
    {
      "source": "/pano-scope/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/pano-scope/$1"
    },
    {
      "source": "/portico/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/portico/$1"
    },
    {
      "source": "/postcards-1001/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/postcards-1001/$1"
    },
    {
      "source": "/search-terms/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/search-terms/$1"
    },
    {
      "source": "/weemala/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/weemala/$1"
    },
    {
      "source": "/youngsydney/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/youngsydney/$1"
    },
    {
      "source": "/visionaries-explorer/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/visionaries-explorer/$1"
    },
    {
      "source": "/feed/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/feed/$1"
    },
    {
      "source": "/young-ct-2018/(.*)",
      "destination": "https://staging-wp.dxlab.sl.nsw.gov.au/young-ct-2018/$1"
    }
  ]
}
